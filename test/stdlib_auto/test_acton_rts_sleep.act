import acton.rts
import time

actor Interruptor():
    def interrupt():
        acton.rts.gc()
        after 0.001: interrupt()
    interrupt()

actor main(env):
    def test():
        t1 = time.time_ns()
        acton.rts.sleep(0.1)
        t2 = time.time_ns()
        diff = (t2-t1) / 1000000000
        if diff < 0.1:
            print("Sleep seems to have been below 1 second :(")
            return 1
        else:
            return 0

#    i = Interruptor()
    r = test()
    env.exit(r)
