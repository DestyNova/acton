
lst = list(range(10, 20, 1))

def even(n):
   return n % 2 == 0
   
actor test(env):
   print("lst =", lst)
   print("list(enumerate(lst,0)) = ",list(enumerate(lst,0)))
   print("list(filter(even,lst)) = ", list(filter(even,lst)))
   print("list(map(even,lst)) = ", list(map(even,lst)))
   chinese = "但他呼吁进行全面调查"
   print(chinese," in ascii is ", ascii(chinese))
   print("max(lst) is ", max(lst,None))
   print("2^32 in hex is ", hex(65536*65536))
   print("chr(8707) is ", chr(8707))
   print("ord('∃') is ", ord("∃"))
   env.exit(0)
   
