lst = list(range(10, 15, 1))

def even(n):
    return n % 2 == 0
    
actor main(env):
    if list(filter(even,lst)) != [10,12,14]:
       print("filter")
       env.exit(1)
    if list(map(even,lst)) != [True,False,True,False,True]:
       print("map")
       env.exit(1)
