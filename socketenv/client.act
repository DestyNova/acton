import stdenv

actor client(env):

    def session(conn):
        if conn is not None:
            conn.on_receipt(input = env.stdout.write,error = env.stdout.write)
            env.stdin.on_input(conn.write)
        else:
            env.stdout.write("Connection failed")
            env.exit(-1)

    env.connect("localhost",12345,session)
