import numpy as np
import math

def harmonic(n):
    sum = 0
    for i in range(1,n+1,1):
        sum += 1/float(i)
    return sum

def harmonic2(n):
    a = np.linspace(1,float(n+1),n)
    return np.sum(1.0/a,0)
  
actor main(env):
    def response(input):
        n = int(input.strip("\n"))
        if n>0:
            print(harmonic2(n))
        else:
            env.exit(0)

    env.stdin_install(response)

