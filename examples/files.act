def main0(env):
   print("will try to open file")
   f1 = env.openW("data")
   print("opened file f1")
   if f1 is not None:
       f1.write("12345\n")
       f1.write("abcde\n")
       f1.close()
       f2 = env.openR("data")
       if f2 is not None:
           s = f2.read()
           print(s)
       else:
           print("cannot open file 'data' for reading")
   else:
       print("cannot open file 'data' for writing")   
   env.exit(0)

actor main(env):
   after 0: main0(env)
   